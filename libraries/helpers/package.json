{
    "name": "@cc-ts/helpers",
    "version": "0.2.1",
    "type": "module",
    "private": false,
    "scripts": {
        "publish": "tstl && changeset publish",
        "build": "tstl",
        "clean-dist": "rm -rf dist",
        "dev": "tstl --watch --declarationMap"
    },
    "files": [
        "dist"
    ],
    "exports": {
        "./*": {
            "types": "./dist/*.d.ts",
            "tstl": "./dist/*"
        }
    },
    "devDependencies": {
        "@types/bun": "latest"
    },
    "peerDependencies": {
        "typescript": "^5.7.2"
    },
    "dependencies": {
        "@jackmacwindows/cc-types": "^1.0.1",
        "@jackmacwindows/craftos-types": "^1.1.3",
        "@jackmacwindows/lua-types": "^2.13.2",
        "@cc-ts/typescript-to-lua": "^1.28.1"
    },
    "publishConfig": {
        "access": "public"
    }
}
